<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-dots="true" autoplay="true" interval="3000">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="355" height="150" />
    </swiper-item>
  </block>
</swiper>
<view class="items-list">
  <view class="pro-item" wx:for="{{proList}}">
    <image class='pro-logo' src='{{item.logo}}'></image>
    <view class="pro-body">
      <view class="pro-title">
      {{item.title}}
      </view>
      <view class="pro-desc">
      {{item.desc}}
      </view>
    </view>
  </view>
</view>